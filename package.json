{
  "name": "nuxt-directus-next",
  "version": "0.0.15",
  "license": "MIT",
  "repository": "https://github.com/Intevel/nuxt-directus/tree/dev",
  "homepage": "https://next.nuxt-directus.site/",
  "type": "module",
  "author": {
    "name": "Conner Luka Bachmann",
    "email": "github@conner-bachmann.de"
  },
  "build": {
    "failOnWarn": false,
    "externals": [
      "defu",
      "ufo"
    ]
  },
  "contributors": [
    "Intevel <github@conner-bachmann.de>",
    "Sandro Circi <sandro.circi@digitoolmedia.com>",
    "Casual Matt <matteo+github@rezzin.ch>"
  ],
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/types.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "docs": "cd docs && pnpm dev",
    "release": "nuxt-module-build build && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@directus/sdk": "^15.1.0",
    "@nuxt/devtools-kit": "^1.0.8",
    "@nuxt/image": "^1.4.0",
    "@nuxt/kit": "^3.9.3",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.8",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@types/node": "^18.19.25",
    "@types/ws": "^8.5.10",
    "changelogen": "^0.4.1",
    "eslint": "^8.57.0",
    "nuxt": "^3.9.3",
    "ohash": "^1.1.3",
    "typescript": "^5.4.2",
    "ufo": "^1.5.2"
  },
  "keywords": [
    "nuxt",
    "directus",
    "nuxtjs",
    "nuxt-module",
    "vue3",
    "nuxt3",
    "directus-module",
    "nuxt-directus",
    "nuxtdirectus",
    "nuxt-community"
  ],
  "packageManager": "pnpm@8.7.6"
}
