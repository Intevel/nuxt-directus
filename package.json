{
  "name": "nuxt-directus-next",
  "version": "0.0.1",
  "license": "MIT",
  "repository": "https://github.com/Intevel/nuxt-directus/tree/dev",
  "homepage": "https://next.nuxt-directus.site/",
  "type": "module",
  "author": {
    "name": "Conner Luka Bachmann",
    "email": "github@conner-bachmann.de"
  },
  "build": {
    "failOnWarn": false,
    "externals": [
      "defu",
      "ofetch",
      "ufo"
    ]
  },
  "contributors": [
    "Intevel <github@conner-bachmann.de>",
    "Sandro Circi <sandro.circi@digitoolmedia.com>",
    "Casual Matt"
  ],
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare playground",
    "docs": "cd docs && pnpm dev",
    "release": "nuxt-module-build && changelogen --release --push && pnpm publish",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@directus/sdk": "^14.0.0",
    "@nuxt/kit": "^3.9.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.6",
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/node": "^18.17.17",
    "@types/ws": "^8.5.8",
    "changelogen": "^0.4.1",
    "eslint": "latest",
    "nuxt": "^3.9.1",
    "ohash": "^1.1.3",
    "typescript": "^5.1.6",
    "ufo": "^1.3.0"
  },
  "keywords": [
    "nuxt",
    "directus",
    "nuxtjs",
    "nuxt-module",
    "vue3",
    "nuxt3",
    "directus-module",
    "nuxt-directus",
    "nuxtdirectus",
    "nuxt-community"
  ],
  "packageManager": "pnpm@8.7.6"
}
